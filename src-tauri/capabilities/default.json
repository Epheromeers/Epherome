{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "os:default",
    "http:default",
    {
      "identifier": "http:allow-fetch",
      "allow": [
        {
          "url": "https://**"
        },
        {
          "url": "http://**"
        }
      ]
    },
    "dialog:default"
  ]
}
